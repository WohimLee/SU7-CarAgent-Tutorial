      "default_action_type": "mixed",
      "children": {
        "navigation_set_destination": {
          "secondary_intent": "navigation_set_destination",
          "cn_name": "设置目的地",
          "action_type": "control",
          "examples": [
            "导航到最近的充电站",
            "带我去公司"
          ]
        },
        "navigation_add_stop": {
          "secondary_intent": "navigation_add_stop",
          "cn_name": "途经点 / 中途加点",
          "action_type": "control",
          "examples": [
            "顺路帮我找个便利店"
          ]
        },
        "navigation_cancel": {
          "secondary_intent": "navigation_cancel",
          "cn_name": "取消导航",
          "action_type": "control",
          "examples": [
            "取消导航"
          ]
        },
        "navigation_poi_search": {
          "secondary_intent": "navigation_poi_search",
          "cn_name": "POI 搜索",
          "action_type": "query",
          "examples": [
            "附近有什么好吃的火锅？"
          ]
