{
  "version": "1.0",
  "intents": {
    "manual": {
      "primary_intent": "manual",
      "cn_name": "手册 / 说明书相关",
      "description": "与车辆使用手册、说明书内容相关的查询类意图",
      "subgraph": "manual_rag",
      "default_action_type": "query",
      "children": {
        "manual_feature_usage": {
          "secondary_intent": "manual_feature_usage",
          "cn_name": "功能使用说明",
          "action_type": "query",
          "examples": [
            "怎么用自适应巡航？"
          ]
        },
        "manual_warning_indicator": {
          "secondary_intent": "manual_warning_indicator",
          "cn_name": "警示灯含义",
          "action_type": "query",
          "examples": [
            "黄色发动机灯亮了什么意思？"
          ]
        },
        "manual_troubleshooting": {
          "secondary_intent": "manual_troubleshooting",
          "cn_name": "故障排查 / 处理",
          "action_type": "query",
          "examples": [
            "窗户关不上了怎么办？"
          ]
        },
        "manual_spec_parameter": {
          "secondary_intent": "manual_spec_parameter",
          "cn_name": "规格 / 参数查询",
          "action_type": "query",
          "examples": [
            "这辆车轮胎胎压标准是多少？"
          ]
        },
        "manual_system_explanation": {
          "secondary_intent": "manual_system_explanation",
          "cn_name": "系统说明 / 模式解释",
          "action_type": "query",
          "examples": [
            "节能模式和普通模式有什么区别？"
          ]
        }
      }
    },
    "vehicle": {
      "primary_intent": "vehicle",
      "cn_name": "车辆状态 / 控制",
      "description": "车辆当前状态查询以及车身各类控制操作",
      "subgraph": "vehicle_control",
      "children": {
        "vehicle_status_query": {
          "secondary_intent": "vehicle_status_query",
          "cn_name": "车辆状态查询",
          "action_type": "query",
          "examples": [
            "现在还有多少电？",
            "车门有没有关好？"
          ]
        },
        "vehicle_control_ac": {
          "secondary_intent": "vehicle_control_ac",
          "cn_name": "空调控制",
          "action_type": "control",
          "examples": [
            "把空调调到 22 度",
            "开大一点风"
          ]
        },
        "vehicle_control_window": {
          "secondary_intent": "vehicle_control_window",
          "cn_name": "车窗控制",
          "action_type": "control",
          "examples": [
            "把驾驶位车窗降一点"
          ]
        },
        "vehicle_control_light": {
          "secondary_intent": "vehicle_control_light",
          "cn_name": "车灯控制",
          "action_type": "control",
          "examples": [
            "打开近光灯"
          ]
        },
        "vehicle_control_seat": {
          "secondary_intent": "vehicle_control_seat",
          "cn_name": "座椅控制",
          "action_type": "control",
          "examples": [
            "帮我加热座椅"
          ]
        },
        "vehicle_control_drive_mode": {
          "secondary_intent": "vehicle_control_drive_mode",
          "cn_name": "驾驶模式切换",
          "action_type": "control",
          "examples": [
            "切到运动模式"
          ]
        },
        "vehicle_control_misc": {
          "secondary_intent": "vehicle_control_misc",
          "cn_name": "其他车辆控制",
          "action_type": "control",
          "examples": [
            "打开雨刮器",
            "打开天窗",
            "打开尾门"
          ]
        }
      }
