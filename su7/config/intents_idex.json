      "subgraph": "media_agent",
      "children": {
        "media_play_music": {
          "secondary_intent": "media_play_music",
          "cn_name": "播放音乐",
          "action_type": "control",
          "examples": [
            "放点轻音乐"
          ]
        },
        "media_play_radio": {
          "secondary_intent": "media_play_radio",
          "cn_name": "播放电台",
          "action_type": "control",
          "examples": [
            "打开 FM 87.6"
          ]
        },
        "media_play_source": {
          "secondary_intent": "media_play_source",
          "cn_name": "切换播放源",
          "action_type": "control",
          "examples": [
            "放我手机里的歌"
          ]
        },
        "media_control": {
          "secondary_intent": "media_control",
          "cn_name": "媒体播放控制",
          "action_type": "control",
          "examples": [
            "暂停一下",
            "下一首"
          ]
        }
