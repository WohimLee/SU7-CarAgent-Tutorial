        "navigation_route_preference": {
          "secondary_intent": "navigation_route_preference",
          "cn_name": "路线偏好设置",
          "action_type": "control",
          "examples": [
            "不要走高速",
            "选一条不堵车的路"
          ]
        }
      }
    },
    "trip": {
      "primary_intent": "trip",
      "cn_name": "行程 / 综合任务",
      "description": "跨子图的综合任务规划，由 Task Planner 进行拆解和编排",
      "subgraph": "task_planner",
      "default_action_type": "plan",
      "children": {
        "trip_plan_route_plus_vehicle": {
          "secondary_intent": "trip_plan_route_plus_vehicle",
          "cn_name": "行程 + 车辆状态综合规划",
          "action_type": "plan",
          "examples": [
            "明天去杭州旅游，帮我规划路线，还要看看电够不够。"
          ]
        },
        "trip_check_before_drive": {
          "secondary_intent": "trip_check_before_drive",
          "cn_name": "出行前车辆检查建议",
          "action_type": "plan",
          "examples": [
            "我要出长途，帮我检查一下车况，需要注意什么？"
          ]
        },
        "trip_charging_plan": {
          "secondary_intent": "trip_charging_plan",
          "cn_name": "长途充电规划",
          "action_type": "plan",
          "examples": [
            "从上海到南京，帮我规划一下充电站。"
          ]
        }
      }
